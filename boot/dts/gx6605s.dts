/dts-v1/;

/ {
    #address-cells = <1>;
    #size-cells = <1>;
    model = "C-SKY gx6605s";

    clocks {
        dummy_apb: apb-clock {
            #clock-cells = <0>;
            compatible = "fixed-clock";
            clock-frequency = <27000000>;
        };
    };

    memory@10000000 {
        device_type = "memory";
        reg = <0x10000000 0x04000000>;
    };

    soc {
        #address-cells = <1>;
        #size-cells = <1>;
        compatible = "amba-bus";
        interrupt-parent = <&intc>;

        timer@20a000 {
            compatible = "csky,gx6605s-timer";
            reg = <0x0020a000 0x400>;
            clocks = <&dummy_apb>;
            interrupts = <10>;
        };

        gpio0: gpio@305000 {
            #gpio-cells = <2>;
            compatible = "wd,mbl-gpio";
            reg-names = "dirout", "dat", "set", "clr";
            reg = <0x305000 4>, <0x305004 4>, <0x305008 4>, <0x30500c 4>;
            gpio-base = <0>;
            gpio-controller;
        };

        serial@403000 {
            compatible = "ns16550a";
            reg = <0x00403000 0x400>;
            interrupts = <15>;
            clock-frequency = <29491200>;
            baud = <115200>;
            reg-shift = <2>;
            reg-io-width = <1>;
        };

        intc: interrupt-controller@500000 {
            #interrupt-cells = <1>;
            #address-cells = <0>;
            compatible = "csky,gx6605s-intc";
            reg = <0x00500000 0x400>;
            interrupt-controller;
        };

        ehci-hcd@900000 {
            compatible = "generic-ehci";
            reg = <0x00900000 0x400>;
            interrupts = <59>;
        };

        ohci-hcd0@a00000 {
            compatible = "generic-ohci";
            reg = <0x00a00000 0x400>;
            interrupts = <58>;
        };

        ohci-hcd1@b00000 {
            compatible = "generic-ohci";
            reg = <0x00b00000 0x400>;
            interrupts = <57>;
        };

        vpu@4800000 {
            compatible = "csky,gx6605s-vpu";
            reg = <0x04800000 0x1000>;
        };
    };

    chosen {
        bootargs = "root=/dev/sda1 init=/sbin/init";
    };
};
